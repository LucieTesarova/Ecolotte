---
layout: layouts/hlavni
---

<section class="product">
    <img src="/images/{{obrazek}}" width="500" alt="foto trika" />
  <div class="product_description">
    <div class="product_texts">
    <h2 class="product_title">{{ title }}</h2>
    <p class="product_text">{{ content | safe }}</p>
    </div>
  <div class="product_order">
    <p class="product_price">{{ cena }}</p>
    <button class="product_button">Objednat</button>
  </div>
  </div>
</section>
